{
    "title":"Dataset",
    "description":"JSON schema for Dataset POJO",
    "implements":[
        {
            "$ref":"org.sagebionetworks.repo.model.Locationable"
        },
        {
            "$ref":"org.sagebionetworks.repo.model.HasLayers"
        },
        {
            "$ref":"org.sagebionetworks.repo.model.HasShortcuts"
        }
        
    ],
    "properties":{
        "species":{
            "type":"string",
            "description":"The species associated with this dataset",
            "title":"Species"
        },
		"disease":{
            "type":"string",
            "description":"The disease associated with this dataset",
            "title":"Disease"
		},
        "eulaId":{
            "type":"string",
            "description":"The id of the EULA for this dataset",
            
        },
        "hasExpressionData":{
            "type":"boolean",
            "description":"Indicates that this dataset contains expression data",
            "transient":true,
            
        },
        "hasGeneticData":{
            "type":"boolean",
            "description":"Indicates that this dataset contains genotype data",
            "transient":true,
            
        },
        "hasClinicalData":{
            "type":"boolean",
            "description":"Indicates that this datasets contains clinical data",
            "transient":true,
            
        },
        "status":{
            "type":"string",
            "description":"the status of this dataset",
            "title":"Status",
        },
        "releaseDate":{
            "type":"string",
            "format":"date-time",
            "title":"Release Date",
        },        
    }
}
