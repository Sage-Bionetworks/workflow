{
    "title":"Data",
    "description":"JSON schema for Data POJO",
    "implements":[
        {
            "$ref":"org.sagebionetworks.repo.model.Locationable"
        },
        {
            "$ref":"org.sagebionetworks.repo.model.HasPreviews"
        }
    ],
    "properties":{
        "releaseNotes":{
            "type":"string",
            "description":"Free text about Data",
            "title":"Release Notes",
            
        },
        "type":{
            "type":"string",
            "description":"Type of Data",
            "name":"LayerTypeNames",
            "id":"org.sagebionetworks.repo.model.LayerTypeNames",
            "enum":[
                "E",
                "G",
                "C",
                "M"
            ],
            
        },
        "tissueType":{
            "type":"string",
            "description":"Type of tissue for the samples in this Data. Tissue is described by the Synapse ontology concept: http://synapse.sagebase.org/ontology#6501",
            "links":[
                {
                    "rel":"describedby",
                    "href":"http://synapse.sagebase.org/ontology#6501",
                    
                },
                
            ],
            "title":"Tissue Type",
            
        },
        "platform":{
            "type":"string",
            "description":"Chip platform for the samples in this Data",
            "links":[
                {
                    "rel":"describedby",
                    "href":"http://synapse.sagebase.org/ontology#6501",
                    
                },
                
            ],
            "title":"Platform",
            
        },
        "numSamples":{
            "type":"integer",
            "description":"Approximate number of samples in this Data",
            "title":"Number of Samples"
        },
		"disease":{
            "type":"string",
            "description":"The disease associated with this Study",
            "title":"Disease"
		},
        "species":{
            "type":"string",
            "description":"The species associated with this Study",
            "title":"Species"
        },
    }
}
